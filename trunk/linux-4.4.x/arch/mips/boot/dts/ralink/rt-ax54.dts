/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>
#include "mt7621-raeth-gmac2.dtsi"
#include "mt7621-pinctrl-default-gpio.dtsi"

/ {
	compatible = "mediatek,mt7621-soc";
	model = "ASUS RT-AX54";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		sys_blue {
			label = "power";
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
		};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;
		};

		reset {
			label = "reset";
			gpios = <&gpio0 16 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio0 15 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};
};

&bch {
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw"; /* temporary, soon change to native nmbm when ported */
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x00000 0x80000>; /* 512k */
			};
			
			partition@80000 {
				label = "u-boot-env";
				reg = <0x80000 0x80000>; /* 512k */
			};
			
			partition@100000 {
				label = "Factory";
				reg = <0x100000 0x100000>; /* 1024k */
			};

			partition@2e0000 {
				label = "factory2";
				reg = <0x2e0000 0x100000>; /* 1024k */
			};

			partition@600000 {
				label = "firmware";
				reg = <0x600000 0x1400000>; /* 20M */
			};

			partition@1a00000 {
				label = "Config";
				reg = <0x1a00000 0x80000>; /* 512K */
			};

			partition@1a80000 {
				label = "Storage";
				reg = <0x1a80000 0x400000>; /* 4096K */
			};

			partition@1e80000 {
				label = "RWFS";
				reg = <0x1e80000 0x51a0000>;
			}; /* 81625K */

			partition@all {
				label = "ALL";
				reg = <0x0 0x7020000>; /* last 8M are reserved for NMBM */
			};
		};
	};
};

&pcie {
	status = "okay";
};
